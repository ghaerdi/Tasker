  "main": "src/main.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve": "node dist/main.js",
    "build": "npm run clean && tsc",
    "watch": "nodemon --files src/main.ts -x ts-node",
    "prepare": "husky install .husky",
    "format": "prettier --write src/**/*.ts",
